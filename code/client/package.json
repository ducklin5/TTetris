{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "options": {
    "allowedHosts": ["localhost", ".localhost"],
    "proxy": "http://localhost:8080"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@ungap/structured-clone": "0.3.4",
    "bootstrap": "4.1.1",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-bootstrap": "2.2.0",
    "react-dom": "17.0.2",
    "react-p5": "1.3.27",
    "react-p5-wrapper": "3.2.1",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "react-test-renderer": "17.0.0",
    "simple-peer": "9.11.1",
    "snapshot-diff": "0.9.0",
    "socket.io-client": "4.4.1",
    "socket.io-mock": "1.3.2",
    "stream": "0.0.2",
    "typescript": "4.6.2",
    "uuid": "8.3.2",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll --coverage --verbose=true --silent=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.jsx",
      "<rootDir>/test/**/*.js",
      "<rootDir>/**/*.test.jsx",
      "<rootDir>/**/*.test.js"
    ],
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
