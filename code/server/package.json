{
  "name": "server",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -e ts,js --exec npx ts-node ./src/index.js",
    "prodStart": "npx ts-node ./src/index.js",
    "test": "node --experimental-modules --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --verbose false",
    "jest:clearcache": "jest --clear-cache"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.3",
    "imurmurhash": "0.1.4",
    "istanbul-reports": "3.1.4",
    "merge-descriptors": "1.0.1",
    "nock": "13.2.4",
    "node-fetch": "3.2.2",
    "random-seed": "0.3.0",
    "socket.io": "4.4.1",
    "socket.io-mock": "1.3.2",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@ungap/structured-clone": "0.3.4",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "snapshot-diff": "0.9.0",
    "snapshot-diff-serializer": "1.0.0",
    "ts-jest": "27.1.3"
  },
  "jest": {
    "transform": {},
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "snapshotSerializers": [
      "snapshot-diff-serializer"
    ]
  }
}
